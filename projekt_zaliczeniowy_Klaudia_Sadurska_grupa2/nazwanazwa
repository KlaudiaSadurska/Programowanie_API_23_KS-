        "#7 Podaj, w który dzień tygodnia najczęściej były publikowane tweety z datasetu\n",
        "\n",
        "projekt_na_programowanie['dzien_tygodnia'] = pd.to_datetime(projekt_na_programowanie['created_at']).dt.day_name()\n",
        "\n",
        "najczestszy_dzien_tygodnia = projekt_na_programowanie['dzien_tygodnia'].value_counts().idxmax()\n",
        "\n",
        "angielskie_dni_na_polskie = {\n",
        "    \"Monday\": \"Poniedziałek\",\n",
        "    \"Tuesday\": \"Wtorek\",\n",
        "    \"Wednesday\": \"Środa\",\n",
        "    \"Thursday\": \"Czwartek\",\n",
        "    \"Friday\": \"Piątek\",\n",
        "    \"Saturday\": \"Sobota\",\n",
        "    \"Sunday\": \"Niedziela\"\n",
        "}\n",
        "\n",
        "najczestszy_dzien_tygodnia_pl = angielskie_dni_na_polskie[najczestszy_dzien_tygodnia]\n",
        "\n",
        "print(\"W ten dzień tygodnia, najczęściej były publikowane tweety: \", najczestszy_dzien_tygodnia_pl)\n"
