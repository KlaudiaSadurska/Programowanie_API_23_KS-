      "cell_type": "code",
      "source": [
        "#2 Rozpoznaj miejsca (placeName) i przekaż je do kolumny o nazwie places\n",
        "\n",
        "def rozpoznaj_miejsca(tekst):\n",
        "    doc = nlp(tekst)\n",
        "    miejsca = [ent.text for ent in doc.ents if ent.label_ == 'placeName']\n",
        "    return ', '.join(miejsca)\n",
        "projekt_na_programowanie['places'] = projekt_na_programowanie['text'].apply(rozpoznaj_miejsca)\n",
        "\n",
        "print(projekt_na_programowanie['places'])"
      ],
