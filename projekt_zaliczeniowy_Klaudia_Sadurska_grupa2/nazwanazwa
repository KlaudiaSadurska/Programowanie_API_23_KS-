      "cell_type": "code",
      "source": [
        "#3 Rozpoznaj organizacje (orgName) i przekaż je do kolumny o nazwie organizations\n",
        "\n",
        "def rozpoznaj_organizacje(text):\n",
        "    doc = nlp(text)\n",
        "    organizacje = [ent.text for ent in doc.ents if ent.label_ == 'orgName']\n",
        "    return ', '.join(organizacje)\n",
        "projekt_na_programowanie['organizations'] = projekt_na_programowanie['text'].apply(rozpoznaj_organizacje)\n",
        "\n",
        "print(projekt_na_programowanie['organizations'])"
