      "cell_type": "code",
      "source": [
        "#1 Rozpoznaj osoby (persName) i przekaż je do kolumny o nazwie persons\n",
        "\n",
        "def rozpoznaj_osoby(tekst):\n",
        "    doc = nlp(tekst)\n",
        "    osoby = [ent.text for ent in doc.ents if ent.label_ == 'persName']\n",
        "    return ', '.join(osoby)\n",
        "projekt_na_programowanie['persons'] = projekt_na_programowanie['text'].apply(rozpoznaj_osoby)\n",
        "\n",
        "print(projekt_na_programowanie['persons'])"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "Y3DRvNxSX3fH",
