      "cell_type": "code",
      "source": [
        "#1 Wykorzystując pakiet matplotlib wykonaj wykres ilustrujący liczbę tweetów per dzień tygodnia\n",
        "\n",
        "projekt_na_programowanie['dzien_tygodnia'] = pd.to_datetime(projekt_na_programowanie['created_at']).dt.day_name()\n",
        "\n",
        "angielskie_dni_na_polskie = {\n",
        "    \"Monday\": \"Poniedziałek\",\n",
        "    \"Tuesday\": \"Wtorek\",\n",
        "    \"Wednesday\": \"Środa\",\n",
        "    \"Thursday\": \"Czwartek\",\n",
        "    \"Friday\": \"Piątek\",\n",
        "    \"Saturday\": \"Sobota\",\n",
        "    \"Sunday\": \"Niedziela\"\n",
        "}\n",
        "\n",
        "projekt_na_programowanie['dzien_tygodnia'] = projekt_na_programowanie['dzien_tygodnia'].map(angielskie_dni_na_polskie)\n",
        "\n",
        "tweety_dzien_tygodnia = projekt_na_programowanie['dzien_tygodnia'].value_counts()\n",
        "\n",
        "kolejnosc_dni = [\"Poniedziałek\", \"Wtorek\", \"Środa\", \"Czwartek\", \"Piątek\", \"Sobota\", \"Niedziela\"]\n",
        "\n",
        "tweety_dzien_tygodnia = tweety_dzien_tygodnia.reindex(kolejnosc_dni)\n",
        "\n",
        "plt.figure(figsize=(12, 6))\n",
        "plt.bar(tweety_dzien_tygodnia.index, tweety_dzien_tygodnia.values, color='pink')\n",
        "plt.xlabel('Dzień tygodnia')\n",
        "plt.ylabel('Liczba tweetów')\n",
        "plt.title('Liczba tweetów per dzień tygodnia')\n",
        "plt.xticks(rotation=45)\n",
        "plt.show()"
